    const directoryList = document.getElementById("directoryList");

    function onShareClick(event, directoryName) {
      event.stopPropagation();

      // Trigger the share menu
      if (navigator.share) {
        navigator.share({
          title: "Check out this game!",
          url: `https://media.tlochsta.dev/game/${directoryName}/index`
        }).catch(error => {
          console.error("Share failed:", error);
        });
      } else {
        console.log("Sharing is not supported in this browser.");
      }
    }

    function onItemClick(directoryName) {
      const url = `https://media.tlochsta.dev/game/${directoryName}/index`;
      const dummyElement = document.createElement("input");
      document.body.appendChild(dummyElement);
      dummyElement.value = url;
      dummyElement.select();
      document.execCommand("copy");
      document.body.removeChild(dummyElement);

      const successIcon = document.createElement("i");
      successIcon.classList.add("fas", "fa-check", "text-success", "ml-2");
      setTimeout(() => {
        directoryList.removeChild(successIcon);
      }, 1000);

      const listItem = document.createElement("li");
      listItem.classList.add("list-group-item", "d-flex", "justify-content-between", "align-items-center");
      listItem.style.cursor = "pointer";

      const nameContainer = document.createElement("div");
      nameContainer.textContent = directoryName;
      nameContainer.classList.add("mr-auto"); // Aligns the game name to the left

      const iconContainer = document.createElement("div");
      iconContainer.classList.add("d-flex", "align-items-center");

      const clipboardIcon = document.createElement("i");
      clipboardIcon.classList.add("far", "fa-clipboard");

      const shareButton = document.createElement("i");
      shareButton.classList.add("fas", "fa-share-from-square", "ml-2");
      shareButton.style.cursor = "pointer";
      shareButton.addEventListener("click", (event) => onShareClick(event, directoryName));

      const playButton = document.createElement("i");
      playButton.classList.add("fas", "fa-play", "ml-2");

      iconContainer.appendChild(clipboardIcon);
      iconContainer.appendChild(shareButton);
      iconContainer.appendChild(playButton);

      listItem.appendChild(nameContainer);
      listItem.appendChild(iconContainer);
      listItem.addEventListener("click", () => onItemClick(directoryName));

      directoryList.appendChild(listItem);
    }

  directoryNames.forEach(directoryName => {
    const listItem = document.createElement("li");
    listItem.classList.add("list-group-item", "d-flex", "justify-content-between", "align-items-center");
    listItem.style.cursor = "pointer";

    const nameContainer = document.createElement("div");
    nameContainer.textContent = directoryName;
    nameContainer.classList.add("mr-auto"); // Aligns the game name to the left

    const iconContainer = document.createElement("div");
    iconContainer.classList.add("d-flex", "align-items-center");

    const clipboardIcon = document.createElement("i");
    clipboardIcon.classList.add("far", "fa-clipboard");

    const shareButton = document.createElement("i");
    shareButton.classList.add("fas", "fa-share-from-square", "ml-2");
    shareButton.style.cursor = "pointer";
    shareButton.addEventListener("click", (event) => onShareClick(event, directoryName));

    const playButton = document.createElement("i");
    playButton.classList.add("fas", "fa-play", "ml-2");

    // Add a bit of spacing between the icons
    iconContainer.appendChild(clipboardIcon);
    iconContainer.appendChild(document.createTextNode("\u00A0\u00A0")); // Non-breaking space
    iconContainer.appendChild(shareButton);
    iconContainer.appendChild(document.createTextNode("\u00A0")); // Non-breaking space
    iconContainer.appendChild(playButton);

    listItem.appendChild(nameContainer);
    listItem.appendChild(iconContainer);
    listItem.addEventListener("click", () => openGameLink(directoryName));

    directoryList.appendChild(listItem);
  });

  function openGameLink(directoryName) {
    window.location.href = `https://media.tlochsta.dev/game/${directoryName}/index`;
  }
